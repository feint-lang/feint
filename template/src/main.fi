# main.fi
#
# "package" is a special name referring to the current package. It's the
# *only* way to access objects in the package.

import package
import from package: Type

# $main is a special name. If it's defined, it will automatically be
# called whenever a module is run as a script.
$main = (argv) ->
    loop arg <- argv
        t = Type.new(arg)
        print(t)

f = (x) ->
    x * x

g = (h) ->
    h()

g((x) -> x)
